{
  "name": "delta.chat-kaios",
  "version": "1.0.0",
  "scripts": {
    "test": "tsc && npm run check-formatting",
    "check-formatting": "prettier --check \"{*.json,*.md,src/**/*.{ts,tsx,scss,html,webapp}}\"",
    "fix-formatting": "prettier --write \"{*.json,*.md,src/**/*.{ts,tsx,scss,html,webapp}}\"",
    "package": "npm run build-prod && ./tools/package",
    "clean": "rm dist/bundle.js",
    "build-dev": "webpack --mode development",
    "build-prod": "webpack --mode production",
    "start": "webpack serve --hot --mode development"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deltachat/delta.chat-kaiOs"
  },
  "license": "GPL-3.0-or-later",
  "authors": [
    "strukturart",
    "simonLaux"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.12.0",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "preact": "^10.4.0",
    "react": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "adm-zip": "^0.4.13",
    "babel-loader": "^8.2.2",
    "copy-webpack-plugin": "^7.0.0",
    "css-loader": "^5.0.1",
    "fs-extra": "^8.1.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "markdown-loader": "^6.0.0",
    "mini-css-extract-plugin": "^1.3.3",
    "node-sass": "^4.13.0",
    "prettier": "^1.19.1",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "svg-sprite-loader": "^5.2.1",
    "ts-loader": "^8.0.12",
    "typescript": "^3.9.7",
    "webpack": "^5.11.0",
    "webpack-bundle-analyzer": "^4.3.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0"
  },
  "staticPath": {
    "source": "/static",
    "target": "/dist"
  }
